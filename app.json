{
  "expo": {
    "name": "SpotifyApp",
    "slug": "spotify-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Spotify.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "androidNavigationBar": {
      "visible": "sticky-immersive",
      "backgroundColor": "#000000"
    },
    "androidStatusBar": {
      "backgroundColor": "#000000"
    },
    "splash": {
      "image": "./assets/StartUp-Screen.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.spotify.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Spotify.png",
        "backgroundColor": "#1DB954"
      },
      "package": "com.spotify.app",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/Spotify.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser"
    ]
  }
}
